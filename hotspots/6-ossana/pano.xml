<krpano>
	
	<!-- Import noce's sking -->
    <include url="../../assets/krpano/skin/noceskin.xml" />
	
	<!-- Import model plugin for schede -->
	<include url="../../assets/krpano/plugins/showtext.xml" />

	<!-- Import gyroscope plugin -->
     <plugin name="gyro" devices="html5" keep="true" url="../../assets/krpano/plugins/gyro2.js" enabled="true" camroll="true" friction="0.0"
        touch_mode="full" sensor_mode="1" softstart="0.5" autocalibration="false" />
        
	<scene name="scene_ossana-castello1" title="ossana-castello1" onstart="" thumburl="panos/ossana-castello1.tiles/thumb.jpg" lat="" lng="" heading="">
		
		<view hlookat="0.0" vlookat="0.0" fovtype="MFOV" fov="120" maxpixelzoom="1.0" fovmin="70" fovmax="140" limitview="auto" />

		<preview url="panos/ossana-castello1.tiles/preview.jpg" />

		<image type="CUBE" multires="true" tilesize="512">
			<level tiledimagewidth="1664" tiledimageheight="1664">
				<cube url="panos/ossana-castello1.tiles/%s/l2/%v/l2_%s_%v_%h.jpg" />
			</level>
			<level tiledimagewidth="768" tiledimageheight="768">
				<cube url="panos/ossana-castello1.tiles/%s/l1/%v/l1_%s_%v_%h.jpg" />
			</level>
		</image>
	</scene>


	<scene name="scene_ossana-plastico" title="ossana-plastico" onstart="" thumburl="panos/ossana-plastico.tiles/thumb.jpg" lat="" lng="" heading="">

		<view hlookat="0.0" vlookat="0.0" fovtype="MFOV" fov="120" maxpixelzoom="1.0" fovmin="70" fovmax="140" limitview="auto" />

		<preview url="panos/ossana-plastico.tiles/preview.jpg" />

		<image type="CUBE" multires="true" tilesize="512">
			<level tiledimagewidth="1664" tiledimageheight="1664">
				<cube url="panos/ossana-plastico.tiles/%s/l2/%v/l2_%s_%v_%h.jpg" />
			</level>
			<level tiledimagewidth="768" tiledimageheight="768">
				<cube url="panos/ossana-plastico.tiles/%s/l1/%v/l1_%s_%v_%h.jpg" />
			</level>
		</image>

	</scene>



	<!-- start Actions-->


	<!-- startup action - load the first scene -->
	<action name="startup" autorun="onstart">
		loadscene(scene_ossana-castello1);
	</action>

	<action name="togglescheda" type="Javascript"><![CDATA[
		toggleScheda();
	]]></action>

	<!-- end Actions-->


</krpano>
