<krpano>
	
	<!-- Import noce's sking -->
    <include url="../../assets/krpano/skin/noceskin.xml" />
	
	<!-- Import model plugin for schede -->
	<include url="../../assets/krpano/plugins/showtext.xml" />

	<!-- Override noce's skin -->
   	<skin_settings thumbs="false"/>

	<!-- Import gyroscope plugin -->
     <plugin name="gyro" devices="html5" keep="true" url="../../assets/krpano/plugins/gyro2.js" enabled="true" camroll="true" friction="0.0"
        touch_mode="full" sensor_mode="1" softstart="0.5" autocalibration="false" />
        
	
		
	<!-- view settings -->
	<view hlookat="180" vlookat="10" maxpixelzoom="1.0" fovmax="150" limitview="auto" />
	

	<preview url="contre-gigapix.tiles/preview.jpg" />


		<!--markerConfluenza -->
        <hotspot name="markerConfluenza-Trigger"
           ath="180" atv="30"
           scale="0.6"
           vr="false"
           type="image" url="../../assets/images/element.png" keep="false" visible="true" enabled="true" handcursor="false" distorted="true" maskchildren="false" edge="center"  zoom="false"  rotate="0.0"  alpha="1.0" borderwidth="2" bordercolor="#0xAAAAAA"
        />

        <hotspot name="markerConfluenza-TriggerLabel"
           html="Confluenza Rabies"
           ath="180" atv="30"
           vr="false"
           oy="50"
           type="text" css="font-family:Arial; font-size:24px; color:#000000;" vcenter="false" padding="2" edge="center" distorted="true" renderer="webgl" wordwrap="true" bg="true" bgcolor="0xFFFFFF" bgalpha="0.5" bgborder="0" bgroundedge="0" bgshadow="" txtshadow="" mergedalpha="true" oversampling="1.0" mipmapping="false" onautosized="" handcursor="false"
        /><!-- Fine markerConfluenza -->



        	<!--contre -->
        <hotspot name="contre-Trigger"
           ath="178" atv="65"
           scale="0.4"
           vr="false"
           onclick="openurl('../18-contre',_self);"		
           type="image" url="../../assets/images/drone-icon.png" keep="false" visible="true" enabled="true" handcursor="true" distorted="true" maskchildren="false" edge="center"  zoom="false"  rotate="0.0"  alpha="1.0" borderwidth="2" bordercolor="#0xAAAAAA"
        />

        <hotspot name="contre-TriggerLabel"
           html="Le Contre"
           ath="178" atv="65"
           vr="false"
           oy="30"
           type="text" css="font-family:Arial; font-size:18px; color:#000000;" vcenter="false" padding="2" edge="center" distorted="true" renderer="webgl" wordwrap="true" bg="true" bgcolor="0xFFFFFF" bgalpha="0.5" bgborder="0" bgroundedge="0" bgshadow="" txtshadow="" mergedalpha="true" oversampling="1.0" mipmapping="false" onautosized=""
        /><!-- Fine contre -->



	<image type="CUBE" prealign="0|0.0|0" multires="true" tilesize="512">
		<level tiledimagewidth="7808" tiledimageheight="7808">
			<cube url="contre-gigapix.tiles/%s/l4/%v/l4_%s_%v_%h.jpg" />
		</level>
		<level tiledimagewidth="3840" tiledimageheight="3840">
			<cube url="contre-gigapix.tiles/%s/l3/%v/l3_%s_%v_%h.jpg" />
		</level>
		<level tiledimagewidth="2048" tiledimageheight="2048">
			<cube url="contre-gigapix.tiles/%s/l2/%v/l2_%s_%v_%h.jpg" />
		</level>
		<level tiledimagewidth="1024" tiledimageheight="1024">
			<cube url="contre-gigapix.tiles/%s/l1/%v/l1_%s_%v_%h.jpg" />
		</level>
	</image>




	<!-- start Actions-->

	<action name="togglescheda" type="Javascript"><![CDATA[
		toggleScheda();
	]]></action>

	<!-- end Actions-->


</krpano>
