<krpano>
	
	<!-- Import noce's sking -->
    <include url="../../assets/krpano/skin/noceskin.xml" />
	
	<!-- Import model plugin for schede -->
	<include url="../../assets/krpano/plugins/showtext.xml" />

	<!-- Import gyroscope plugin -->
     <plugin name="gyro" devices="html5" keep="true" url="../../assets/krpano/plugins/gyro2.js" enabled="true" camroll="true" friction="0.0"
        touch_mode="full" sensor_mode="1" softstart="0.5" autocalibration="false" />
        
	
	<!-- start Scene 1 -->
		<scene name="scene_palu-acqua" title="palu-acqua" onstart="" thumburl="panos/palu-acqua.tiles/thumb.jpg" lat="" lng="" heading="">

		<view hlookat="0.0" vlookat="0.0" fovtype="MFOV" fov="120" maxpixelzoom="1.0" fovmin="70" fovmax="140" fisheye="1.0" limitview="auto" />

		<preview url="panos/palu-acqua.tiles/preview.jpg" />

		<image type="CUBE" multires="true" tilesize="512">
			<level tiledimagewidth="2176" tiledimageheight="2176">
				<cube url="panos/palu-acqua.tiles/%s/l2/%v/l2_%s_%v_%h.jpg" />
			</level>
			<level tiledimagewidth="1152" tiledimageheight="1152">
				<cube url="panos/palu-acqua.tiles/%s/l1/%v/l1_%s_%v_%h.jpg" />
			</level>
		</image>

	</scene>

	<scene name="scene_palu-diga" title="palu-diga" onstart="" thumburl="panos/palu-diga.tiles/thumb.jpg" lat="" lng="" heading="">

		<view hlookat="160.0" vlookat="0.0" fovtype="MFOV" fov="120" maxpixelzoom="1.0" fovmin="70" fovmax="140" fisheye="1.0" limitview="auto" />

		<preview url="panos/palu-diga.tiles/preview.jpg" />

		<image type="CUBE" multires="true" tilesize="512">
			<level tiledimagewidth="2176" tiledimageheight="2176">
				<cube url="panos/palu-diga.tiles/%s/l2/%v/l2_%s_%v_%h.jpg" />
			</level>
			<level tiledimagewidth="1152" tiledimageheight="1152">
				<cube url="panos/palu-diga.tiles/%s/l1/%v/l1_%s_%v_%h.jpg" />
			</level>
		</image>
		
		<hotspot name="scene_palu-maso"
			 onclick="loadscene('scene_palu-maso', null, MERGE);"
			 ath="130.0" atv="-4.0"
			 scale="0.15"
			 rotate="0.0"
			type="image" url="../../assets/images/arrow.png" keep="false" visible="true" enabled="true" handcursor="true" distorted="true" maskchildren="false" edge="center" zoom="false" borderwidth="2" bordercolor="#0xAAAAAA" alpha="1.0"/>
		<hotspot name="scene_palu-strada"
			 onclick="loadscene('scene_palu-strada', null, MERGE);"
			 ath="250.0" atv="0.0"
			 scale="0.15"
			 rotate="0.0"
			type="image" url="../../assets/images/arrow.png" keep="false" visible="true" enabled="true" handcursor="true" distorted="true" maskchildren="false" edge="center" zoom="false" borderwidth="2" bordercolor="#0xAAAAAA" alpha="1.0"/>
		<hotspot name="scene_palu-acqua"
			 onclick="loadscene('scene_palu-acqua', null, MERGE);"
			 ath="180.0" atv="-5.0"
			 scale="0.15"
			 rotate="0.0"
			type="image" url="../../assets/images/arrow.png" keep="false" visible="true" enabled="true" handcursor="true" distorted="true" maskchildren="false" edge="center" zoom="false" borderwidth="2" bordercolor="#0xAAAAAA" alpha="1.0"/>
			

	</scene>

	<scene name="scene_palu-lago" title="palu-lago" onstart="" thumburl="panos/palu-lago.tiles/thumb.jpg" lat="" lng="" heading="">

		<view hlookat="45.0" vlookat="0.0" fovtype="MFOV" fov="140" maxpixelzoom="1.0" fovmin="70" fovmax="140" fisheye="1.0" limitview="auto" />

		<preview url="panos/palu-lago.tiles/preview.jpg" />

		<!--scheda-dighe -->
        <hotspot name="scheda-dighe-Trigger"
           ath="20" atv="0"
           scale="0.8"
           vr="false"
           onclick="jscall(loadScheda('scheda-dighe'));"           
           type="image" url="../../assets/images/element.png" keep="false" visible="true" enabled="true" handcursor="true" distorted="true" maskchildren="false" edge="center"  zoom="false"  rotate="0.0"  alpha="1.0" borderwidth="2" bordercolor="#0xAAAAAA"
        />

        <hotspot name="scheda-dighe-TriggerLabel"
           html="Le dighe"
           ath="20" atv="0"
           vr="false"
           oy="50"
           type="text" css="font-family:Arial; font-size:24px; color:#000000;" vcenter="false" padding="2" edge="center" distorted="true" renderer="webgl" wordwrap="true" bg="true" bgcolor="0xFFFFFF" bgalpha="0.5" bgborder="0" bgroundedge="0" bgshadow="" txtshadow="" mergedalpha="true" oversampling="1.0" mipmapping="false" onautosized=""
        /><!-- Fine scheda-dighe -->
		



		<image type="CUBE" multires="true" tilesize="512">
			<level tiledimagewidth="2176" tiledimageheight="2176">
				<cube url="panos/palu-lago.tiles/%s/l2/%v/l2_%s_%v_%h.jpg" />
			</level>
			<level tiledimagewidth="1152" tiledimageheight="1152">
				<cube url="panos/palu-lago.tiles/%s/l1/%v/l1_%s_%v_%h.jpg" />
			</level>
		</image>
		<hotspot name="scene_palu-maso"
			 onclick="loadscene('scene_palu-maso', null, MERGE);"
			 ath="80.0" atv="-5.0"
			 scale="0.15"
			 rotate="0.0"
			type="image" url="../../assets/images/arrow.png" keep="false" visible="true" enabled="true" handcursor="true" distorted="true" maskchildren="false" edge="center" zoom="false" borderwidth="2" bordercolor="#0xAAAAAA" alpha="1.0"/>
	</scene>

	<scene name="scene_palu-maso" title="palu-maso" onstart="" thumburl="panos/palu-maso.tiles/thumb.jpg" lat="" lng="" heading="">

		<view hlookat="90.0" vlookat="0.0" fovtype="MFOV" fov="120" maxpixelzoom="1.0" fovmin="70" fovmax="140" fisheye="1.0" limitview="auto" />

		<preview url="panos/palu-maso.tiles/preview.jpg" />

		<image type="CUBE" multires="true" tilesize="512">
			<level tiledimagewidth="2176" tiledimageheight="2176">
				<cube url="panos/palu-maso.tiles/%s/l2/%v/l2_%s_%v_%h.jpg" />
			</level>
			<level tiledimagewidth="1152" tiledimageheight="1152">
				<cube url="panos/palu-maso.tiles/%s/l1/%v/l1_%s_%v_%h.jpg" />
			</level>
		</image>
		<hotspot name="scene_palu-lago"
			 onclick="loadscene('scene_palu-lago', null, MERGE);"
			 ath="80.0" atv="5.0"
			 scale="0.15"
			 rotate="0.0"
			type="image" url="../../assets/images/arrow.png" keep="false" visible="true" enabled="true" handcursor="true" distorted="true" maskchildren="false" edge="center" zoom="false" borderwidth="2" bordercolor="#0xAAAAAA" alpha="1.0"/>
		<hotspot name="scene_palu-diga"
			 onclick="loadscene('scene_palu-diga', null, MERGE);"
			 ath="110.0" atv="3.0"
			 scale="0.15"
			 rotate="0.0"
			type="image" url="../../assets/images/arrow.png" keep="false" visible="true" enabled="true" handcursor="true" distorted="true" maskchildren="false" edge="center" zoom="false" borderwidth="2" bordercolor="#0xAAAAAA" alpha="1.0"/>
			
			

	</scene>

	<scene name="scene_palu-strada" title="palu-strada" onstart="" thumburl="panos/palu-strada.tiles/thumb.jpg" lat="" lng="" heading="">

		<view hlookat="100.0" vlookat="0.0" fovtype="MFOV" fov="120" maxpixelzoom="1.0" fovmin="70" fovmax="140" fisheye="1.0" limitview="auto" />

		<preview url="panos/palu-strada.tiles/preview.jpg" />

		<image type="CUBE" multires="true" tilesize="512">
			<level tiledimagewidth="2176" tiledimageheight="2176">
				<cube url="panos/palu-strada.tiles/%s/l2/%v/l2_%s_%v_%h.jpg" />
			</level>
			<level tiledimagewidth="1152" tiledimageheight="1152">
				<cube url="panos/palu-strada.tiles/%s/l1/%v/l1_%s_%v_%h.jpg" />
			</level>
		</image>
		<hotspot name="scene_palu-diga"
			 onclick="loadscene('scene_palu-diga', null, MERGE);"
			 ath="75.0" atv="3.0"
			 scale="0.15"
			 rotate="0.0"
			type="image" url="../../assets/images/arrow.png" keep="false" visible="true" enabled="true" handcursor="true" distorted="true" maskchildren="false" edge="center" zoom="false" borderwidth="2" bordercolor="#0xAAAAAA" alpha="1.0"/>

	</scene>



	<!-- start Actions-->


	<!-- startup action - load the first scene -->
	<action name="startup" autorun="onstart">
		loadscene(scene_palu-lago, null, MERGE);
	</action>

	<action name="togglescheda" type="Javascript"><![CDATA[
		toggleScheda();
	]]></action>

	<!-- end Actions-->


</krpano>
